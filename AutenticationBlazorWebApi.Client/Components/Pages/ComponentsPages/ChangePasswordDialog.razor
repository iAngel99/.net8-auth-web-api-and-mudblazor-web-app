
<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6" Align="Align.Center">Cambiar contraseña</MudText>
    </TitleContent>
    <DialogContent>
        <MudText>Correo: @parameters.Email</MudText>
            <MudTextField @bind-Value="parameters.NewPassword" For="@(() => parameters.NewPassword)" Label="Nueva contraseña" InputType="InputType.Password" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancelar">Cancelar</MudButton>
        <MudButton Color="Color.Primary" OnClick="GuardarContraseña">Guardar</MudButton>
    </DialogActions>
</MudDialog>